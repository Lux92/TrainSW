<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Stazione" table="stazione"/>
   <id name="id_stazione" column="id_stazione">
         <generator class="increment"/>
      </id>
      <property name="num_bin" column="num_bin"/>
      <property name="locazione" column="locazione"/>
      <property name="nome_stazione" column="nome_stazione"/>
</hibernate-mapping>
